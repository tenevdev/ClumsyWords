<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ClumsyWordsUniversal.Common">

    <Color x:Key="ClumsyOrange">#FFFA6800</Color>
    <Color x:Key="ClumsyOrangeLight">#FFFBD480</Color>
    <Color x:Key="ClumsyBlue">#FF0091FA</Color>
    <Color x:Key="ClumsyBlueLight">#FF80C8FD</Color>

    <SolidColorBrush x:Key="ClumsyOrangeBrush" Color="{StaticResource ClumsyOrange}"/>


</ResourceDictionary>
